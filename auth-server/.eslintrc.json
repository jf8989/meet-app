{
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "globals": {
    "exports": "writable"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "require-await": "warn"
  }
}
